{
  "schema_version": 1,
  "quilt_loader": {
    "group": "com.kneelawk",
    "id": "ears-editor",
    "version": "${version}",
    "metadata": {
      "name": "Ears Editor",
      "description": "Simple mod to allow me to edit the Ears features I want that are not generally accessible.",
      "contributors": {
        "Kneelawk": "Owner"
      },
      "contact": {
        "homepage": "https://kneelawk.com/",
        "issues": "https://github.com/Kneelawk/ears-editor/issues",
        "sources": "https://github.com/Kneelawk/ears-editor"
      },
      "icon": "assets/ears-editor/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": "com.kneelawk.earseditor.EarsEditorModKt::init"
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.17.0-"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=4.0.0-"
      },
      {
        "id": "ears",
        "versions": "^1.4"
      },
      {
        "id": "libgui",
        "versions": "^6.1"
      },
      {
        "id": "minecraft",
        "versions": ">=1.19.2"
      }
    ]
  },
  "mixin": "ears-editor.mixins.json"
}
